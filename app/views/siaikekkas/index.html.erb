# coding: utf-8
<h1>試合結果一覧</h1>

<table>
  <tr>
    <th>大会</th>
    <th>日付</th>
    <th>対戦カード</th>
    <th>最終スコア</th>
    <th>グランド</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @siaikekkas.each do |siaikekka| %>
  <tr>
    <td><%= siaikekka.taikaimei %></td>
    <td><%= siaikekka.hiduke %></td>
    <td><%= siaikekka.taisenaite %></td>
    <td><%= siaikekka.score %></td>
    <td><%= siaikekka.grand %></td>
    <td><%= link_to 'Show', siaikekka %></td>
    <td><%= link_to 'Edit', edit_siaikekka_path(siaikekka) %></td>
    <td><%= link_to 'Destroy', siaikekka, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

